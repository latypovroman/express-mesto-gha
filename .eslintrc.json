{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "extends:airbnb-base",
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
      "no-underscore-dangle": [
        "error",
        {
          "allow": ["_id"]
        }
      ]
    }
}
